# Keycloak OIDC Settings
OIDC_REALM=myrealm
OIDC_CLIENT_ID=oauth2-proxy
OIDC_CLIENT_SECRET=your-client-secret
OIDC_ISSUER_URL=https://keycloak:8443/realms/myrealm
OIDC_AUTH_URL=http://localhost:18080/realms/myrealm/protocol/openid-connect/auth
OIDC_REDEEM_URL=https://keycloak:8443/realms/myrealm/protocol/openid-connect/token
OIDC_PROFILE_URL=https://keycloak:8443/realms/myrealm/protocol/openid-connect/userinfo
OIDC_JWKS_URL=https://keycloak:8443/realms/myrealm/protocol/openid-connect/certs

# OAuth2 Proxy
# Generate with: python -c 'import os,base64; print(base64.urlsafe_b64encode(os.urandom(32)).decode())'
OAUTH2_PROXY_COOKIE_SECRET=your-cookie-secret
OAUTH2_PROXY_REDIRECT_URL=http://localhost:8000/oauth2/callback

# URLs (ブラウザからアクセスするベース URL)
KEYCLOAK_PUBLIC_URL=http://localhost:18080
APP_BASE_URL=http://localhost:8000
